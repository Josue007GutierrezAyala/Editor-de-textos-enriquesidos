<Window x:Class="Textos_enriquesidos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Textos_enriquesidos"
        mc:Ignorable="d"
        Title="Editor de textos enriquesidos" Height="450" Width="800">
    <Window.CommandBindings>
        <CommandBinding Command="ApplicationCommands.Open" Executed="Open_Excuted"/>
        <CommandBinding Command="ApplicationCommands.Save" Executed="Save_Excuted"/>
    </Window.CommandBindings>
    <DockPanel>
        <ToolBar DockPanel.Dock="Top">
            <Button Command="ApplicationCommands.Open">
                <Image Source="Resources/folder.jpg" Width="16" Height="16"/>
            </Button>
            <Button Command="ApplicationCommands.Save">
                <Image Source="Resources/disk.png" Width="16" Height="16"/>
            </Button>
        </ToolBar>
    </DockPanel>
    
</Window>
